{
  "manifest_version": 3,
  "name": "EtherXProxy",
  "version": "1.3",
  "description": "Redirect specific URLs to new destinations, proxy required to participate in EtherX'24.",
  "icons": {
    "48": "assets/48.png",
    "128": "assets/128.png"
  },
  "permissions": [
    "declarativeNetRequest"
  ],
  "host_permissions": [
    "*://*.etherx.com/*",
    "*://*.theeye.com/*"
  ],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset_1",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  }
}
